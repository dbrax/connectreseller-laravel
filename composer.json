{
    "name": "epmnzava/connectreseller-laravel",
    "description": "This is a laravel library for domain management with connectresseler",
    "keywords": [
        "epmnzava",
        "connectreseller-laravel"
    ],
    "homepage": "https://github.com/dbrax/connectreseller-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Emmanuel Mnzava",
            "email": "epmnzava@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0|^7.0|^8.0"

    },

    "autoload": {
        "psr-4": {
            "Epmnzava\\ConnectresellerLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Epmnzava\\ConnectresellerLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Epmnzava\\ConnectresellerLaravel\\ConnectresellerLaravelServiceProvider"
            ],
            "aliases": {
                "ConnectresellerLaravel": "Epmnzava\\ConnectresellerLaravel\\ConnectresellerLaravelFacade"
            }
        }
    }
}
